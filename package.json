{
    "name": "node-restream",
    "version": "0.0.1",
    "description": "This free Node.js + FFmpeg + nginx-rtmp-module based restream app (like restream.io).",
    "repository": {
        "type": "git",
        "url": "https://github.com/nikakoy131/node-restream.git"
    },
    "main": "server.js",
    "scripts": {
        "test": "test",
        "start": "nodemon server.js"
    },
    "author": "Ruslan Beliy <nikakoy131@gmail.com>",
    "license": "ISC",
    "eslintConfig": {
        "extends": [
            "airbnb-base",
            "prettier"
        ],
        "env": {
            "node": true
        },
        "rules": {
            "indent": [
                "error",
                4
            ],
            "eol-last": [
                "error",
                "never"
            ],
            "object-shorthand": [
                "error",
                "never"
            ],
            "prefer-template": 0,
            "comma-dangle": [
                "error",
                "never"
            ]
        }
    },
    "devDependencies": {
        "eslint": "^4.14.0",
        "eslint-config-airbnb-base": "^12.1.0",
        "eslint-config-prettier": "^2.9.0",
        "eslint-plugin-import": "^2.8.0",
        "nodemon": "^1.11.0"
    },
    "dependencies": {
        "assert": "^1.4.1",
        "body-parser": "^1.17.2",
        "express": "^4.15.3",
        "mongodb": "^2.2.29",
        "uuid": "^3.1.0"
    }
}
